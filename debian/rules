#!/usr/bin/make -f

%:
	dh $@ --with gir

override_dh_auto_configure:
	dh_auto_configure -- --libdir=\$${prefix}/lib/$(DEB_HOST_MULTIARCH) \
	                     --enable-compile-warnings=yes \
	                     --enable-gupnp \
	                     --with-gstreamer \
	                     --enable-introspection

override_dh_install:
	dh_install --list-missing

override_dh_auto_test:
